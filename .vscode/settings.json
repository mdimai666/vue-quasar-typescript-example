{
  "vetur.format.defaultFormatter.html": "prettier",
  "vetur.format.defaultFormatter.js": "vscode-typescript",
  "vetur.format.defaultFormatter.ts": "vscode-typescript",
  "files.watcherExclude": {
    "dist": true
  },
  "files.exclude": {
    "dist": true
  },
  "vetur.experimental.templateInterpolationService": true,
  "vetur.validation.template": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    {
      "language": "vue",
      "autoFix": true
    },
    "javascript",
    "javascriptreact",
    "typescript",
    {
      "language": "vue",
      "autoFix": true
    }
  ],
  "typescript.format.semicolons": "remove",
  "attrsSorter.order": [
    "id",
    "is",
    "v-if",
    "v-else-if",
    "v-else",
    "v-show",
    "v-for",
    "v-cloak",
    "v-once",
    "v-pre",
    "ref",
    "key",
    "v-slot",
    "v-slot.+",
    "#.*",
    "slot",
    "v-model",
    "v-model.+",
    "v-bind",
    "v-bind.+",
    ":.+",
    "v-text",
    "v-text.+",
    "v-html",
    "v-html.+",
    "class",
    "v-on.+",
    "@.+",
    "name",
    "data-.+",
    "ng-.+",
    "src",
    "for",
    "type",
    "href",
    "values",
    "title",
    "alt",
    "role",
    "aria-.+",
    "$unknown$"
  ],
  // "editor.fontFamily": "'Iosevka SS07', monospace"
}